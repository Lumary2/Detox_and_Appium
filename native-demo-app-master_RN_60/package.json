{
  "name": "wdio-native-demo-app",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean": "react-native-clean-project",
    "prepush": "npm run lint",
    "clear.cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro* && rm -rf $TMPDIR/haste-*",
    "android.clear.build": "yarn clear.cache && rm -rf ./android/app/build",
    "android.release": "yarn android.clear.build && cd android && ./gradlew assembleRelease",
    "ios.clear.build": "yarn clear.cache && rm -rf ./ios/build/wdioDemoApp/Build/Products/Release-iphonesimulator",
    "ios.sim.release": "yarn ios.clear.build && react-native run-ios --configuration Release",
    "postinstall": "npx jetify",
    "android.sauce.rdc.app": "./node_modules/.bin/wdio ./config/saucelabs/wdio.android.rdc.app.conf.js",
    "android.app": "./node_modules/.bin/wdio ./config/wdio.android.app.conf.js",
    "android.browser": "./node_modules/.bin/wdio ./config/wdio.android.browser.conf.js",
    "ios.app": "./node_modules/.bin/wdio ./config/wdio.ios.app.conf.js",
    "ios.sauce.rdc.app": "./node_modules/.bin/wdio ./config/saucelabs/wdio.ios.rdc.app.conf.js",
    "ios.browser": "./node_modules/.bin/wdio ./config/wdio.ios.browser.conf.js",
    "lint": "eslint config tests"
  },
  "dependencies": {
    "@tele2/react-native-select-input": "^1.1.0",
    "i18n-js": "^3.3.0",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-elements": "^1.2.0",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-image-zoom-viewer": "^2.2.27",
    "react-native-localize": "^1.2.0",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^2.0.0-alpha.3",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^7.0.5",
    "react-navigation": "^4.0.5",
    "react-navigation-tabs": "^2.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.6.0",
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "@wdio/appium-service": "^5.16.11",
    "@wdio/cli": "^5.18.1",
    "@wdio/jasmine-framework": "^5.16.15",
    "@wdio/local-runner": "^5.18.0",
    "@wdio/sauce-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.17.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-wdio": "^5.13.2",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "node-fetch": "^2.6.0",
    "react-native-clean-project": "^3.2.4",
    "react-test-renderer": "16.8.6",
    "webdriverio": "^5.22.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
